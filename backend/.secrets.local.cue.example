// Local secrets for development
// This file should NOT be committed to version control
//
// Copy this file to .secrets.local.cue and fill in your actual API keys:
//   cp .secrets.local.cue.example .secrets.local.cue
//
// Or use Encore CLI to set secrets:
//   encore secret set --type local DeepgramAPIKey
//   encore secret set --type local OpenAIAPIKey
//   encore secret set --type local FirecrawlAPIKey
//   encore secret set --type local SupabaseJWTSecret

DeepgramAPIKey: "your-deepgram-api-key-here"
OpenAIAPIKey: "your-openai-api-key-here"
FirecrawlAPIKey: "your-firecrawl-api-key-here"
SupabaseJWTSecret: "your-supabase-jwt-secret-here"
